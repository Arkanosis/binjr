<?xml version="1.0" encoding="UTF-8"?>

<?import eu.fthevenet.binjr.commons.charts.ZonedDateTimeAxis?>
<?import eu.fthevenet.binjr.commons.controls.ZonedDateTimePicker?>
<?import eu.fthevenet.binjr.commons.fxchart.StableTicksAxis?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.AreaChart?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Polyline?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<AnchorPane fx:id="root" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.fthevenet.binjr.controllers.TimeSeriesController">
    <VBox prefHeight="537.0" prefWidth="687.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <VBox alignment="CENTER_RIGHT" prefHeight="56.0" prefWidth="687.0">
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                <Button fx:id="backButton" contentDisplay="GRAPHIC_ONLY" graphicTextGap="1.0" maxHeight="30.0" maxWidth="30.0" minHeight="30.0" minWidth="30.0" mnemonicParsing="false"
                        onAction="#handleHistoryBack" prefHeight="30.0" prefWidth="30.0" text="back" textAlignment="CENTER">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0"/>
                    </HBox.margin>
                    <graphic>
                        <ImageView fitHeight="14" fitWidth="14">
                            <Image url="@/images/backArrow.png"/>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="Go back">
                            <font>
                                <Font size="10.0"/>
                            </font>
                        </Tooltip>
                    </tooltip>
                </Button>
                <Button fx:id="forwardButton" contentDisplay="GRAPHIC_ONLY" graphicTextGap="1.0" maxHeight="30.0" maxWidth="30.0" minHeight="30.0" minWidth="30.0" mnemonicParsing="false"
                        onAction="#handleHistoryForward" prefHeight="30.0" prefWidth="30.0" text="forward" textAlignment="CENTER">
                    <graphic>
                        <ImageView fitHeight="14" fitWidth="14">
                            <Image url="@/images/forwardArrow.png"/>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="Go forward">
                            <font>
                                <Font size="10.0"/>
                            </font>
                        </Tooltip>
                    </tooltip>
                </Button>
                <Button fx:id="refreshButton" contentDisplay="GRAPHIC_ONLY" graphicTextGap="1.0" maxHeight="30.0" maxWidth="30.0" minHeight="30.0" minWidth="30.0" mnemonicParsing="false"
                        onAction="#handleRefresh" prefHeight="30.0" prefWidth="30.0" text="refresh" textAlignment="CENTER">
                    <graphic>
                        <ImageView fitHeight="14" fitWidth="14">
                            <Image url="@/images/refresh.png"/>
                        </ImageView>
                    </graphic>
                    <font>
                        <Font size="1.0"/>
                    </font>
                    <tooltip>
                        <Tooltip text="Refresh">
                            <font>
                                <Font size="10.0"/>
                            </font>
                        </Tooltip>
                    </tooltip>
                </Button>
                <Separator orientation="VERTICAL" prefHeight="200.0">
                    <HBox.margin>
                        <Insets left="10.0"/>
                    </HBox.margin>
                </Separator>
                <VBox alignment="CENTER_LEFT">
                    <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                        <VBox.margin>
                            <Insets bottom="5.0" left="5.0" right="1.0" top="5.0"/>
                        </VBox.margin>
                        <Label text="From:"/>
                        <ZonedDateTimePicker fx:id="startDate" prefHeight="25.0" prefWidth="150.0">
                            <tooltip>
                                <Tooltip text="Lower bound for the X axis"/>
                            </tooltip>
                        </ZonedDateTimePicker>
                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                        <Label prefHeight="17.0" prefWidth="26.0" text="Min:"/>
                        <TextField fx:id="yMinRange" editable="true" prefHeight="25.0" prefWidth="130.0">
                            <tooltip>
                                <Tooltip text="Lower bound for the Y axis"/>
                            </tooltip>
                        </TextField>
                    </HBox>
                    <HBox alignment="CENTER_LEFT" fillHeight="false" spacing="10.0" VBox.vgrow="NEVER">
                        <VBox.margin>
                            <Insets bottom="5.0" left="5.0" right="2.0" top="5.0"/>
                        </VBox.margin>
                        <Label prefHeight="17.0" prefWidth="30.0" text="To:"/>
                        <ZonedDateTimePicker fx:id="endDate" prefHeight="25.0" prefWidth="150.0">
                            <tooltip>
                                <Tooltip text="Upper bound for the X axis"/>
                            </tooltip>
                        </ZonedDateTimePicker>
                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                        <Label text="Max:"/>
                        <TextField fx:id="yMaxRange" editable="true" prefHeight="25.0" prefWidth="130.0">
                            <tooltip>
                                <Tooltip text="Upper bound for the Y axis"/>
                            </tooltip>
                        </TextField>
                    </HBox>
                </VBox>
                <VBox alignment="CENTER_LEFT" HBox.hgrow="NEVER">
                    <Polyline stroke="#b5b5b5" strokeType="INSIDE">
                        <points>
                            <Double fx:value="0.0"/>
                            <Double fx:value="0.0"/>
                            <Double fx:value="0.0"/>
                            <Double fx:value="-7.0"/>
                            <Double fx:value="-15.0"/>
                            <Double fx:value="-7.0"/>
                        </points>
                    </Polyline>
                    <Button fx:id="resetYButton" contentDisplay="GRAPHIC_ONLY" maxHeight="21.0" maxWidth="21.0" minHeight="21.0" minWidth="21.0" mnemonicParsing="false"
                            onAction="#handleResetYRangeButton" prefHeight="21.0" prefWidth="21.0" textOverrun="CLIP">
                        <graphic>
                            <ImageView fitHeight="15" fitWidth="11">
                                <Image url="@/images/autoScale.png"/>
                            </ImageView>
                        </graphic>
                        <font>
                            <Font size="1.0"/>
                        </font>
                        <VBox.margin>
                            <Insets bottom="2.0" left="4.0" right="10.0" top="2.0"/>
                        </VBox.margin>
                        <tooltip>
                            <Tooltip text="Automatically set the range for the Y axis"/>
                        </tooltip>
                    </Button>
                    <Polyline stroke="#b5b5b5" strokeType="INSIDE">
                        <points>
                            <Double fx:value="0.0"/>
                            <Double fx:value="7.0"/>
                            <Double fx:value="15.0"/>
                            <Double fx:value="7.0"/>
                            <Double fx:value="15.0"/>
                            <Double fx:value="0.0"/>
                        </points>
                    </Polyline>
                </VBox>
                <Separator orientation="VERTICAL" prefHeight="200.0"/>
                <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" spacing="10.0"/>
            </HBox>
        </VBox>
        <SplitPane dividerPositions="0.6949" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" orientation="VERTICAL" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets/>
            </VBox.margin>
            <AnchorPane minHeight="0.0" minWidth="0.0">
                <cursor>
                    <Cursor fx:constant="DEFAULT"/>
                </cursor>
                <AnchorPane fx:id="chartParent" prefHeight="429.0" prefWidth="670.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <AreaChart fx:id="chart" animated="false" createSymbols="false" focusTraversable="true" legendVisible="false" prefHeight="431.0" prefWidth="675.0"
                               AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <xAxis>
                            <ZonedDateTimeAxis animated="false" side="BOTTOM"/>
                        </xAxis>
                        <yAxis>
                            <StableTicksAxis animated="false" autoRanging="true" side="LEFT" tickSpacing="30"/>
                        </yAxis>
                    </AreaChart>
                </AnchorPane>
            </AnchorPane>
            <AnchorPane>
                <ListView fx:id="seriesList" layoutY="-1.0" prefHeight="132.0" prefWidth="685.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                          AnchorPane.topAnchor="0.0"/>
            </AnchorPane>
        </SplitPane>
    </VBox>
</AnchorPane>
