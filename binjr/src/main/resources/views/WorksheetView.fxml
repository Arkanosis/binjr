                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0"/>
                    </HBox.margin>
                    <graphic>
                        <ImageView fitHeight="14" fitWidth="14">
                            <Image url="@/images/backArrow.png"/>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="Go back">
                            <font>
                                <Font size="10.0"/>
                            </font>
                        </Tooltip>
                    </tooltip>
                </Button>
                <Button fx:id="forwardButton" contentDisplay="GRAPHIC_ONLY" graphicTextGap="1.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                        mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" text="forward" textAlignment="CENTER">
                    <graphic>
                        <ImageView fitHeight="14" fitWidth="14">
                            <Image url="@/images/forwardArrow.png"/>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="Go forward">
                            <font>
                                <Font size="10.0"/>
                            </font>
                        </Tooltip>
                    </tooltip>
                </Button>
                <Button fx:id="refreshButton" contentDisplay="GRAPHIC_ONLY" graphicTextGap="1.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                        mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" text="refresh" textAlignment="CENTER">
                    <graphic>
                        <ImageView fitHeight="14" fitWidth="14">
                            <Image url="@/images/refresh.png"/>
                        </ImageView>
                    </graphic>
                    <font>
                        <Font size="10.0"/>
                    </font>
                    <tooltip>
                        <Tooltip text="Refresh">
                            <font>
                                <Font size="10.0"/>
                            </font>
                        </Tooltip>
                    </tooltip>
                </Button>
                <Separator orientation="VERTICAL" prefHeight="200.0">
                    <HBox.margin>
                        <Insets left="10.0"/>
                    </HBox.margin>
                </Separator>
                <GridPane hgap="10.0" vgap="10.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES"/>
                        <ColumnConstraints hgrow="SOMETIMES"/>
                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"/>
                        <ColumnConstraints hgrow="SOMETIMES"/>
                        <ColumnConstraints hgrow="SOMETIMES"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints vgrow="SOMETIMES"/>
                        <RowConstraints vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <ZonedDateTimePicker fx:id="startDate" GridPane.columnIndex="1">
                            <tooltip>
                                <Tooltip text="Lower bound for the X axis"/>
                            </tooltip>
                        </ZonedDateTimePicker>
                        <ZonedDateTimePicker fx:id="endDate" GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <tooltip>
                                <Tooltip text="Upper bound for the X axis"/>
                            </tooltip>
                        </ZonedDateTimePicker>
                        <Label text="From:"/>
                        <Label text="To:" GridPane.rowIndex="1"/>
                        <TextField fx:id="yMinRange" editable="true" GridPane.columnIndex="4">
                            <tooltip>
                                <Tooltip text="Lower bound for the Y axis"/>
                            </tooltip>
                        </TextField>
                        <TextField fx:id="yMaxRange" editable="true" GridPane.columnIndex="4" GridPane.rowIndex="1">
                            <tooltip>
                                <Tooltip text="Upper bound for the Y axis"/>
                            </tooltip>
                        </TextField>
                        <Label text="Min:" GridPane.columnIndex="3"/>
                        <Label text="Max:" GridPane.columnIndex="3" GridPane.rowIndex="1"/>
                        <Separator orientation="VERTICAL" prefHeight="200.0" GridPane.columnIndex="2"/>
                        <Separator orientation="VERTICAL" prefHeight="200.0" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                    </children>
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" top="5.0"/>
                    </HBox.margin>
                </GridPane>
                <VBox alignment="CENTER_LEFT" HBox.hgrow="NEVER">
                    <Polyline stroke="#b5b5b5" strokeType="INSIDE">
                        <points>
                            <Double fx:value="0.0"/>
                            <Double fx:value="0.0"/>
                            <Double fx:value="0.0"/>
                            <Double fx:value="-7.0"/>
                            <Double fx:value="-15.0"/>
                            <Double fx:value="-7.0"/>
                        </points>
                    </Polyline>
                    <Button fx:id="resetYButton" contentDisplay="GRAPHIC_ONLY" maxHeight="21.0" maxWidth="21.0" minHeight="21.0" minWidth="21.0" mnemonicParsing="false" prefHeight="21.0"
                            prefWidth="21.0" textOverrun="CLIP">
                        <graphic>
                            <ImageView fitHeight="15" fitWidth="11">
                                <Image url="@/images/autoScale.png"/>
                            </ImageView>
                        </graphic>
                        <font>
                            <Font size="1.0"/>
                        </font>
                        <VBox.margin>
                            <Insets bottom="2.0" left="4.0" right="10.0" top="2.0"/>
                        </VBox.margin>
                        <tooltip>
                            <Tooltip text="Automatically set the range for the Y axis">
                                <font>
                                    <Font size="10.0"/>
                                </font>
                            </Tooltip>
                        </tooltip>
                    </Button>
                    <Polyline stroke="#b5b5b5" strokeType="INSIDE">
                        <points>
                            <Double fx:value="0.0"/>
                            <Double fx:value="7.0"/>
                            <Double fx:value="15.0"/>
                            <Double fx:value="7.0"/>
                            <Double fx:value="15.0"/>
                            <Double fx:value="0.0"/>
                        </points>
                    </Polyline>
                    <HBox.margin>
                        <Insets left="2.0"/>
                    </HBox.margin>
                </VBox>
                <Separator orientation="VERTICAL" prefHeight="200.0"/>
                <Button fx:id="snapshotButton" contentDisplay="GRAPHIC_ONLY" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                        prefHeight="30.0" prefWidth="30.0" text="Snapshot">
                    <HBox.margin>
                        <Insets left="5.0"/>
                    </HBox.margin>
                    <graphic>
                        <ImageView fitHeight="14" fitWidth="14">
                            <Image url="@/images/snapshot.png"/>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="Take a snapshot of the worksheet">
                            <font>
                                <Font size="10.0"/>
                            </font>
                        </Tooltip>
                    </tooltip>
                </Button>
                <ToggleButton fx:id="hCrosshair" contentDisplay="GRAPHIC_ONLY" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                              prefHeight="30.0" prefWidth="30.0" text="ToggleButton">
                    <graphic>
                        <ImageView fitHeight="14" fitWidth="14">
                            <Image url="@/images/lockCrosshairHorizontal.png"/>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="Toggle horizontal crosshair marker">
                            <font>
                                <Font size="10.0"/>
                            </font>
                        </Tooltip>
                    </tooltip>
                </ToggleButton>
                <ToggleButton fx:id="vCrosshair" contentDisplay="GRAPHIC_ONLY" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                              prefHeight="30.0" prefWidth="30.0" text="ToggleButton">
                    <graphic>
                        <ImageView fitHeight="14" fitWidth="14">
                            <Image url="@/images/lockCrosshairVertical.png"/>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="Toggle vertical crosshair marker">
                            <font>
                                <Font size="10.0"/>
                            </font>
                        </Tooltip>
                    </tooltip>
                </ToggleButton>
                <Slider fx:id="graphOpacitySlider" blockIncrement="0.1" max="1.0" value="0.8"/>
            </HBox>
        </VBox>
        <SplitPane dividerPositions="0.6949" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" orientation="VERTICAL" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets/>
            </VBox.margin>
            <AnchorPane minHeight="0.0" minWidth="0.0">
                <cursor>
                    <Cursor fx:constant="DEFAULT"/>
                </cursor>
                <AnchorPane fx:id="chartParent" prefHeight="429.0" prefWidth="670.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <!--<AreaChart fx:id="chart" animated="false" createSymbols="false" focusTraversable="true" legendVisible="false" prefHeight="431.0" prefWidth="675.0" AnchorPane.bottomAnchor="0.0"-->
                    <!--AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">-->
                    <!--<xAxis>-->
                    <!--<ZonedDateTimeAxis animated="false" side="BOTTOM"/>-->
                    <!--</xAxis>-->
                    <!--<yAxis>-->
                    <!--<StableTicksAxis animated="false" autoRanging="true" side="LEFT" tickSpacing="30"/>-->
                    <!--</yAxis>-->
                    <!--</AreaChart>-->
                </AnchorPane>
            </AnchorPane>
            <AnchorPane>
                <children>
                    <SplitMenuButton mnemonicParsing="false" text="SplitMenuButton">
                        <items>
                            <MenuItem mnemonicParsing="false" text="Action 1"/>
                            <MenuItem mnemonicParsing="false" text="Action 2"/>
                        </items>
                    </SplitMenuButton>
                    <TableView fx:id="seriesTable" layoutX="125.0" layoutY="-22.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                               AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <columns>
                            <TableColumn fx:id="visibleColumn" prefWidth="26.0"/>
                            <TableColumn fx:id="colorColumn" minWidth="-Infinity" prefWidth="32.0" resizable="false">
                                <!--<cellFactory>-->
                                <!--<ColorTableCellFactory/>-->
                                <!--</cellFactory>-->
                                <!--<cellValueFactory>-->
                                <!--<PropertyValueFactory property="color"/>-->
                                <!--</cellValueFactory>-->
                            </TableColumn>
                            <TableColumn prefWidth="160.0" text="Name">
                                <cellValueFactory>
                                    <PropertyValueFactory property="name"/>
                                </cellValueFactory>
                            </TableColumn>
                            <TableColumn prefWidth="76.0" text="Min.">
                                <cellValueFactory>
                                    <PropertyValueFactory property="minValue"/>
                                </cellValueFactory>
                                <cellFactory>
                                    <DecimalFormatTableCellFactory alignment="RIGHT" pattern="###,###.##"/>
                                </cellFactory>
                            </TableColumn>

                            <TableColumn prefWidth="76.0" text="Avg.">
                                <cellValueFactory>
                                    <PropertyValueFactory property="averageValue"/>
                                </cellValueFactory>
                                <cellFactory>
                                    <DecimalFormatTableCellFactory alignment="RIGHT" pattern="###,###.##"/>
                                </cellFactory>
                            </TableColumn>
                            <TableColumn text="Max.">
                                <cellValueFactory>
                                    <PropertyValueFactory property="maxValue"/>
                                </cellValueFactory>
                                <cellFactory>
                                    <DecimalFormatTableCellFactory alignment="RIGHT" pattern="###,###.##"/>
                                </cellFactory>
                            </TableColumn>
                            <TableColumn prefWidth="75.0" text="Current">
                                <cellFactory>
                                    <DecimalFormatTableCellFactory alignment="RIGHT" pattern="###,###.##"/>
                                </cellFactory>
                            </TableColumn>

                            <TableColumn fx:id="sourceColumn" prefWidth="200.0" text="Source">
                            </TableColumn>
                        </columns>
                        <!--<contextMenu>-->
                        <!--<ContextMenu fx:id="seriesListMenu">-->
                        <!--<items>-->
                        <!--<MenuItem fx:id="removeSeriesMenuItem" mnemonicParsing="false" onAction="#handleRemoveSeries" text="Remove series" disable="true"/>-->
                        <!--</items>-->
                        <!--</ContextMenu>-->
                        <!--</contextMenu>-->
                    </TableView>
                </children>
            </AnchorPane>
        </SplitPane>
    </VBox>
</AnchorPane>
